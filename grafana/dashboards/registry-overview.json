{
  "title": "Registry Overview",
  "uid": "registry-overview",
  "version": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "panels": [
    {
      "title": "Registered Gateways (Current)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "registry_registered_gateways_total",
          "legendFormat": "Gateways"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "title": "gRPC Success Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(registry_grpc_requests_total{result=\"success\"}[5m])) / sum(rate(registry_grpc_requests_total[5m])) * 100",
          "legendFormat": "success %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 95, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "title": "Heartbeat Forwarding Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(registry_grpc_requests_total{method=\"Gateway.Heartbeat\"}[1m]))",
          "legendFormat": "forwards/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 2,
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "title": "Registered Gateways Over Time",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "registry_registered_gateways_total",
          "legendFormat": "Gateways"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "min": 0
        }
      }
    },
    {
      "title": "gRPC Requests by Method & Result",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "sum by (method, result) (rate(registry_grpc_requests_total[1m]))",
          "legendFormat": "{{method}} {{result}}"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 3,
          "min": 0
        }
      }
    },
    {
      "title": "gRPC Latency (p50 / p95 / p99)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket[30s])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket[30s])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket[30s])))",
          "legendFormat": "p99"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0
        }
      }
    },
    {
      "title": "gRPC Latency by Method (p95)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, method) (rate(registry_grpc_request_duration_seconds_bucket[30s])))",
          "legendFormat": "{{method}}"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0
        }
      }
    },
    {
      "title": "Gateway Heartbeats Received (Rate)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum by (result) (rate(registry_grpc_requests_total{method=\"Registry.Heartbeat\"}[1m]))",
          "legendFormat": "{{result}}"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 3,
          "min": 0
        }
      }
    },
    {
      "title": "Heartbeat Forwards to Gateways (Rate)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "sum by (result) (rate(registry_grpc_requests_total{method=\"Gateway.Heartbeat\"}[1m]))",
          "legendFormat": "{{result}}"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 3,
          "min": 0
        }
      }
    },
    {
      "title": "Heartbeat Forward Latency (p95)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket{method=\"Gateway.Heartbeat\"}[30s])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket{method=\"Gateway.Heartbeat\"}[30s])))",
          "legendFormat": "p99"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0
        }
      }
    },
    {
      "title": "Registry Heartbeat Processing Latency (p95)",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket{method=\"Registry.Heartbeat\"}[30s])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(registry_grpc_request_duration_seconds_bucket{method=\"Registry.Heartbeat\"}[30s])))",
          "legendFormat": "p99"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "fillGradient": 0,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "min": 0
        }
      }
    },
    {
      "title": "Heartbeat Flow Summary",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 36 },
      "targets": [
        {
          "expr": "sum by (method, result) (rate(registry_grpc_requests_total[1m]))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "method"
            },
            "properties": [
              { "id": "custom.width", "value": 200 }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "result"
            },
            "properties": [
              { "id": "custom.width", "value": 100 }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              { "id": "unit", "value": "req/s" },
              { "id": "decimals", "value": 3 }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          { "desc": true, "displayName": "Value" }
        ]
      }
    }
  ]
}


